// The Causal Graph
digraph {
	age [label=age]
	workclass [label=workclass]
	education [label=education]
	marital_status [label=marital_status]
	occupation [label=occupation]
	race [label=race]
	gender [label=gender]
	hours_per_week [label=hours_per_week]
	income [label=income]
	age -> education [color=black]
	age -> hours_per_week [color=black]
	gender -> workclass [color=black]
	workclass -> income [color=black]
	education -> income [color=black]
	race -> marital_status [color=black]
	marital_status -> income [color=black]
	race -> occupation [color=black]
	occupation -> income [color=black]
	race -> income [color=black]
	gender -> income [color=black]
	hours_per_week -> income [color=black]
}
